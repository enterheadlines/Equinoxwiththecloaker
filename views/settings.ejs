<body style="background-image: url(./assets/img/bg/bg.svg);">
   <div class="topbar">
      <div class="left-buttons">
         <a id="exit" onclick="exit()" style="display: none">
             <span style="font-weight: bold;"><i class="fas fa-sign-out-alt"></i> Exit</span>
         </a>
         <a id="homebtn" href="/" class="<%= path === '/' ? 'nav-active' : 'nav-btn' %>">
             <span><i class="fas fa-home"></i> Home</span>
         </a>
         <a id="games" href="/g" class="<%= path === '/g' ? 'nav-active' : 'nav-btn' %>">
             <span><i class="fas fa-gamepad"></i> Games</span>
         </a>
         <a id="apps" href="/a" class="<%= path === '/a' ? 'nav-active' : 'nav-btn' %>">
             <span><i class="fas fa-th"></i> Apps</span>
         </a>
     </div>
	
		<form id="form" class="center-form">
         <input autofocus autocomplete="off" class="search" id="search" placeholder="Equinox Settings" style="text-align: center;" disabled />
			<button type="submit" class="submit-button">
				<i class="fas fa-search searchicon"></i>
			</button>
		</form>
	
		<div class="right-section">
		   <a id="util" href="/u" class="<%= path === '/u' ? 'nav-active' : 'nav-btn' %>">
			  <span><i class="fas fa-wrench"></i> Utilities</span>
		  </a>
			<a class="<%= path === '/s' ? 'nav-active' : 'nav-btn' %>" id="settings" href="/s" style="display: flex; justify-content: flex-end; margin-left: auto;">
				<span><i class="fas fa-cog"></i> Settings</span>
			</a>
			<a id="fullscreen" onclick="fullscreen()" style="display: none">
				<span><i class="fas fa-expand"></i> Full Screen</span>
			</a>
		</div>
	</div>
   <div class="games-body">
      <h3 class="category-heading" id="recent-txt"><img class="recent-img" src="/assets/eqsettings.png"
            style="padding-left: 10px"></h3>
      <article class="card">
         <article class="grid gap-big">
            <div class="title">
               Search Settings
               <p>Settings related to Equinox Search</p>
            </div>
            <div class="grid col-2">
               <fieldset>
                  <label>Select a search engine</label>
                  <select id="engine" class="input-group" name="engine">
                     <option value="google">Google</option>
                     <option value="duckduckgo">DuckDuckGo</option>
                     <option value="yahoo">Yahoo</option>
                  </select>
               </fieldset>
            </div>
         </article>
      </article>
      <article class="card">
         <article class="grid gap-big">
            <div class="title">
               Preferences & Security
               <p>Customize your experience on Equinox</p>
            </div>
            <div class="grid col-2">
               <fieldset>
                  <label>
                     Tab Cloaking
                     <p>Change the tab icon and title of the page.</p>
                  </label>
                  <div class="button-group">
                     <button type="button" id="google-cloak" onclick="cloakPage('google')" class="cloak-button"><img
                           class="cloak-img" src="/assets/img/cloak/google.webp"></button>
                     <button type="button" id="docs-cloak" onclick="cloakPage('docs')" class="cloak-button"><img
                           class="cloak-img" src="/assets/img/cloak/google-docs.webp"></button>
                     <button type="button" id="drive-cloak" onclick="cloakPage('drive')" class="cloak-button"><img
                           class="cloak-img" src="/assets/img/cloak/google-drive.webp"></button>
                     <button type="button" id="classroom-cloak" onclick="cloakPage('classroom')"
                        class="cloak-button"><img class="cloak-img" src="/assets/img/cloak/home.webp"></button>
                     <button type="button" id="chrome-cloak" onclick="cloakPage('chrome')" class="cloak-button"><img
                           class="cloak-img" src="/assets/img/cloak/chrome.webp"></button>


                  </div>
                  <div class="button-group">
                     <button type="button" class="button" id="none-cloak" onclick="cloakPage('none')">Reset</button>
                  </div>
               </fieldset>
               <fieldset>
                  <label>
                     Stealth Mode
                     <p>Coming soon...</p>
                  </label>

               </fieldset>
            </div>
         </article>
      </article>
      <article class="card">
         <article class="grid gap-big">
            <div class="title">
               Version Details
               <p>Relevant system and build information of Equinox</p>
            </div>
            <div class="grid col-2">
               <fieldset>
                  <label>
                     Version Number
                     <p id="eqversion">2.0<span id="latestorno"> (Latest)</span></p>
                  </label>
                  <label>
                     Build Number
                     <p id="buildnumber"></p>
                  </label>
                  <label>
                     Server
                     <p>US-01</p>
                  </label>
               </fieldset>
               <fieldset>
                  <label>
